             pMenu.onclick = function(mN, iN) { with (this)
{
 // Do actions depending on the item that the mouse was over at the time of the click.
 // You may with to use nested IFs or 'switch' statements etc. if you're familiar with JS.
 if (mN == 'root')
 {
   
    if (iN == 1 ) location.href = '/forums/menu1.html;jsessionid=aP8HfPphcUch';
    if (iN == 2 ) location.href = '/simple_shopping/menu3.html;jsessionid=aP8HfPphcUch';
    if (iN == 3 ) location.href = '/projects/menu5.html;jsessionid=aP8HfPphcUch';
    if (iN == 4 ) location.href = '/campaigns/menu7.html;jsessionid=aP8HfPphcUch';
    if (iN == 5 ) location.href = '/login/menu9.html;jsessionid=aP8HfPphcUch';
    if (iN == 6 ) location.href = '/downloads/menu13.html;jsessionid=aP8HfPphcUch';
