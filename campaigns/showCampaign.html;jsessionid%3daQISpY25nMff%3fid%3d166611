             pMenu.onclick = function(mN, iN) { with (this)
{
 // Do actions depending on the item that the mouse was over at the time of the click.
 // You may with to use nested IFs or 'switch' statements etc. if you're familiar with JS.
 if (mN == 'root')
 {
   
    if (iN == 1 ) location.href = '/forums/menu1.html;jsessionid=aQISpY25nMff';
    if (iN == 2 ) location.href = '/simple_shopping/menu3.html;jsessionid=aQISpY25nMff';
    if (iN == 3 ) location.href = '/projects/menu5.html;jsessionid=aQISpY25nMff';
    if (iN == 4 ) location.href = '/campaigns/menu7.html;jsessionid=aQISpY25nMff';
    if (iN == 5 ) location.href = '/login/menu9.html;jsessionid=aQISpY25nMff';
    if (iN == 6 ) location.href = '/downloads/menu13.html;jsessionid=aQISpY25nMff';
    if (iN == 7 ) location.href = '/gallery/menu15.html;jsessionid=aQISpY25nMff';
    if (iN == 8 ) location.href = '/blogs/menu17.html;jsessionid=aQISpY25nMff';
    if (iN == 9 ) location.href = '/syndication/menu19.html;jsessionid=aQISpY25nMff';
    if (iN == 10 ) location.href = '/faqs/menu23.html;jsessionid=aQISpY25nMff';
    if (iN == 11 ) location.href = '/cms/Articles.html;jsessionid=aQISpY25nMff';
    if (iN == 12 ) location.href = '/cms/contactus.html;jsessionid=aQISpY25nMff';
    if (iN == 13 ) location.href = '/cms/Links.html;jsessionid=aQISpY25nMff';
    if (iN == 14 ) location.href = '/cms/Merchandise.html;jsessionid=aQISpY25nMff';

 }
